@using ABCo.Multicam.UI.ViewModels.Features.Switcher.Types;
@inherits VMBoundComponent<DummySwitcherConfigMixBlockVM>

<div class="content">
	<div class="index">
		<p>@VM.Index</p>
	</div>
	<p class="tag">Input Count:</p>
	<select class="combo" @bind=VM.InputCount>
		@foreach (var item in VM.InputCountItems)
		{
			<option>@item</option>
		}
	</select>
</div>